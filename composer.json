{
    "name": "iam-adty/pixel-color",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "IamAdty\\PixelColor\\": "src/",
            "Frontpack\\": "vendor_override/Frontpack/"
        },
        "exclude-from-classmap": [
            "vendor/frontpack/composer-assets-plugin/src/RefreshAssetsCommand.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "atk4/ui": "^5.0",
        "nikic/fast-route": "^1.3",
        "kcs/class-finder": "^0.4.0",
        "symfony/string": "^7.0",
        "monolog/monolog": "^3.5",
        "psr/cache": "2",
        "symfony/cache": "^7.0",
        "intervention/image": "^3.4"
    },
    "require-dev": {
        "frontpack/composer-assets-plugin": "^0.11.1"
    },
    "config": {
        "allow-plugins": {
            "frontpack/composer-assets-plugin": true
        }
    },
    "extra": {
        "opus": {
            "enabled": true
        },
        "assets-dir": "public/assets/vendor",
        "assets-files": {
            "atk4/ui": [
                "public/logo.png",
                "public/css",
                "public/js",
                "public/external"
            ]
        }
    }
}
